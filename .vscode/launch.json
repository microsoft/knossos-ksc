// A launch configuration that compiles the extension and then opens it inside a new window
// Use IntelliSense to learn about possible attributes.
// Hover to view descriptions of existing attributes.
// For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
{
	"version": "0.2.0",
	"configurations": [

		{
			"name": "Python: onnx2ks",
			"type": "python",
			"request": "launch",
			"program": "src/onnx2ks/onnx2ks.py",
			"args": [
				"test/onnx2k/super_resolution.onnx", 
				"obj/"
			],
			"env" : {
				"PYTHONPATH": "./src/python"
			},
			"console": "integratedTerminal",
			"justMyCode": false
		},
		{
			"name": "Python: onnx-build-prelude",
			"type": "python",
			"request": "launch",
			"program": "src/onnx2ks/onnx-build-prelude.py",
			"args": [],
			"env" : {
				"PYTHONPATH": "./src/python"
			},
			"console": "integratedTerminal",
			"justMyCode": false
		},
		{
			"name": "Python: Current File",
			"type": "python",
			"request": "launch",
			"program": "${file}",
			"args": [
			],
			"env" : {
				"PYTHONPATH": "./src/python"
			},
			"console": "integratedTerminal",
			"justMyCode": false
		},
		{
			"name": "Run Extension",
			"type": "extensionHost",
			"request": "launch",
			"runtimeExecutable": "${execPath}",
			"args": [
				"--extensionDevelopmentPath=${workspaceFolder}/etc/ks-vscode/"
			],
			"outFiles": [
				"${workspaceFolder}/etc/ks-vscode/out/**/*.js"
			],
			"preLaunchTask": "npm-watch"
		},
		{
			"name": "C++ ksc-mlir",
			"cwd": "${workspaceRoot}",
			"type": "cppdbg",
			"request": "launch",
			"program": "${workspaceFolder}/build/bin/ksc-mlir",
			"_args": [
				"TEST"
			],
			"args": [
				"LLVM",
				"${workspaceFolder}/mlir/test/Ksc/tuple.ks"
			],
			"_miDebuggerPath": "/usr/bin/gdb",
			"MIMode": "gdb",
			"setupCommands": [
				{
					"description": "Enable pretty-printing for gdb",
					"text": "-enable-pretty-printing",
					"ignoreFailures": true
				}
			],
			"externalConsole": false,
			"logging": {
				"moduleLoad": false,
				"trace": true
			}
		},
		{
			"name": "lldb ksc-mlir",
			"cwd": "${workspaceRoot}",
			"type": "lldb",
			"request": "launch",
			"program": "${workspaceFolder}/build/bin/ksc-mlir",
			"args": "LLVM ${workspaceFolder}/mlir/test/Ksc/tuple.ks"
		}
	]
}