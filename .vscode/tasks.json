// for the documentation about the tasks.json format
{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "shell",
			"label": "npm-watch",
			"command": "npm run watch",
			"problemMatcher": "$tsc-watch",
			"isBackground": true,
			"options": {
				"cwd": "${workspaceFolder}/etc/ks-vscode"
			},
			"group": {
				"kind": "build",
				"isDefault": true
			}
		}
	]
}